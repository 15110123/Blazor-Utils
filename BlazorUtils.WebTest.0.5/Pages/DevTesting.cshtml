            course = new Course("132", "Tran Van B")
        };

        Dev.Map(this, student, "mappedStudent");

        Console.WriteLine("Done mapping");
    }

    protected override void OnAfterRender()
    {
        base.OnAfterRender();
        Dev.Warn("After render from component");
    }
}